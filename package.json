{
  "name": "static-theme-misipage",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:giscus-theme": "sass ./src/styles/theme/giscus:./public/assets/styles/giscus/themes --no-source-map",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src/",
    "predev": "npm run build:giscus-theme",
    "prebuild": "npm run build:giscus-theme"
  },
  "engines": {
    "node": ">=16.14.0",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.2.0",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "tailwindcss": "^3.3.1",
    "vite": "^4.2.1",
    "vite-plugin-html": "^3.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "alpinejs": "^3.12.0"
  }
}
